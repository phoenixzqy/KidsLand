{
  "name": "kidsland",
  "private": true,
  "version": "1.0.0",
  "description": "A fun PWA for kids to learn English words through games and quizzes",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fix:tailwind": "find src -name '*.tsx' -o -name '*.ts' | xargs sed -i '' 's/bg-gradient-to-/bg-linear-to-/g; s/from-\\[/from-\\[/g; s/to-\\[/to-\\[/g; s/via-\\[/via-\\[/g'",
    "preview": "vite preview"
  },
  "dependencies": {
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "workbox-window": "^7.4.0"
